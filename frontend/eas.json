{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://language-partner-backend.fly.dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://language-partner-backend.fly.dev"
      }
    },
    "production": {
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://language-partner-backend.fly.dev"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "milsteinprogram@cornell.edu",
        "appleTeamId": "L7GQ3AE2L4"
      }
    }
  }
}
